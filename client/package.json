{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.4",
    "classnames": "^2.2.6",
    "formik": "^1.5.2",
    "immer": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-gravatar": "^2.6.3",
    "react-icons": "^3.6.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "react-transition-group": "^4.0.0",
    "recharts": "^1.5.0",
    "typeface-roboto": "^0.0.54",
    "uuid": "^3.3.2",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel _watch:css _start:react",
    "build": "npm-run-all _build:css _build:react",
    "prod": "NODE_ENV=production node server/index.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "_start:react": "sleep 2 && react-scripts start",
    "_build:react": "react-scripts build",
    "_build:css": "postcss src/styles/index.src.css -o src/styles/index.css",
    "_watch:css": "postcss src/styles/index.src.css -o src/styles/index.css -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint-plugin-prettier": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "postcss-color-function": "^4.1.0",
    "prettier": "1.16.4",
    "react-test-renderer": "^16.8.6",
    "tailwindcss": "^0.7.4"
  }
}
